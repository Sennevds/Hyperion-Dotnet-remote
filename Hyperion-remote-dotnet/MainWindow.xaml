<Window x:Class="Hyperion_remote_dotnet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <TextBox x:Name="IP"></TextBox>
            <TextBox x:Name="Port">22</TextBox>
            <TextBox x:Name="Username"></TextBox>
            <TextBox x:Name="Password"></TextBox>
            <Button x:Name="login" Click="Login_OnClick"></Button>
            <ListBox x:Name="output" SelectionChanged="Output_OnSelectionChanged"></ListBox>
        </StackPanel>
        
    </Grid>
</Window>
